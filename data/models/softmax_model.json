{
  "model_name": "softmax_model",
  "layers" : [
    {
      "type": "input",
      "input_shape": 30
    },
    {
      "type": "dense",
      "units": 16,
      "activation": "lrelu",
      "kernel_initializer": "he_normal"
    },
    {
      "type": "dropout",
      "rate": 0.2
    },
    {
      "type": "dense",
      "units": 16,
      "activation": "tanh",
      "kernel_initializer": "glorot_uniform"
    },
    {
      "type": "dropout",
      "rate": 0.3
    },
    {
      "type": "dense",
      "units": 2,
      "activation": "softmax",
      "kernel_initializer": "glorot_uniform"
    }
  ],
    "optimizer": {
        "type": "adam",
        "learning_rate": 0.0001
    },
    "loss": "binary_crossentropy",
    "batch_size": 512,
    "epochs": 10000
}